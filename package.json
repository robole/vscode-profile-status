{
  "name": "profile-status",
  "displayName": "Profile Status",
  "description": "Know which your profile you're using, no doubt",
  "publisher": "robole",
  "author": {
    "name": "Rob O'Leary"
  },
  "version": "0.4.0",
  "preview": false,
  "engines": {
    "vscode": "^1.46.0"
  },
  "icon": "img/logo.png",
  "galleryBanner": {
    "color": "#ffeda7",
    "theme": "light"
  },
  "bugs": {
    "url": "https://github.com/robole/vscode-profile-status/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/robole/vscode-profile-status"
  },
  "sponsor": {
    "url": "https://ko-fi.com/roboleary"
  },
  "license": "Apache License 2.0",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [],
    "configuration": {
      "type": "object",
      "title": "Profile Status",
      "properties": {
        "profileStatus.itemAlignment": {
          "type": "string",
          "default": "Left",
          "enum": [
            "Left",
            "Right"
          ],
          "description": "Set the position on the status bar."
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "format": "npx prettier --write .",
    "test": "node ./test/runTest.js",
    "dev": "webpack --mode development --watch",
    "vscode:prepublish": "webpack --mode production",
    "build": "vsce package"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^10.0.1",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.46.0",
    "eslint": "^7.32.0",
    "eslint-config-node-roboleary": "^0.1.0",
    "glob": "^7.1.6",
    "mocha": "^10.2.0",
    "typescript": "^4.1.3",
    "vsce": "^2.15.0",
    "vscode-test": "^1.5.0",
    "webpack": "^5.80.0",
    "webpack-cli": "^5.0.1"
  }
}
